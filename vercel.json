{
    "version": 2,
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "outputDirectory": "dist",
    "framework": "vite",
    "routes": [
      { "src": "/(.*)", "dest": "/index.html" }
    ],
    "headers": [
      {
        "source": "/assets/(.*)",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/javascript"
          }
        ]
      },
      {
        "source": "/assets/index-CPkJDmQH.css",
        "headers": [
          {
            "key": "Content-Type", 
            "value": "text/css"
          }
        ]
      }
    ],
    "build": {
      "env": {
        "VITE_BUILD": "true"
      }
    }
  }