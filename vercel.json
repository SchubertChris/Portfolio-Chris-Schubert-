{
    "version": 2,
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "outputDirectory": "dist",
    "framework": "vite",
    "headers": [
      {
        "source": "/assets/(.*)\\.js",
        "headers": [
          { "key": "Content-Type", "value": "application/javascript" }
        ]
      },
      {
        "source": "/assets/(.*)\\.css",
        "headers": [
          { "key": "Content-Type", "value": "text/css" }
        ]
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }